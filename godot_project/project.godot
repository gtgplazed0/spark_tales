; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="SparkTales"
config/description="A learning game for children with social limitations. "
run/main_scene="res://Scenes/main.tscn"
config/features=PackedStringArray("4.2", "Mobile")

[audio]

general/text_to_speech=true

[autoload]

Passwords="*res://Scripts/passwords.gd"
SoundFx="*res://Scenes/sound_fx.tscn"
TextToSpeech="*res://Scenes/text_to_speech.tscn"
FileOpener="*res://Scripts/file_dialogue.gd"
DataScript="*res://Scripts/data_script.gd"

[debug]

gdscript/warnings/unused_variable=0

[display]

window/size/viewport_width=1180
window/size/viewport_height=810
window/size/mode=3
window/size/initial_position_type=0
window/size/extend_to_title=true
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[filesystem]

import/blender/enabled=false

[input]

R-KEY={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":82,"key_label":0,"unicode":114,"echo":false,"script":null)
]
}

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
